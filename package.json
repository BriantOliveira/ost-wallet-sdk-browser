    "bignumber.js": "8.1.1",
    "bip39": "3.0.2",
    "ethereumjs-abi": "0.6.8",
    "ethereumjs-util": "6.2.0",
    "ethereumjs-wallet": "0.6.3",
    "eventemitter3": "4.0.0",
    "form-data": "3.0.0",
    "handlebars": "4.7.2",
    "lodash": "4.17.4",
    "moment-timezone": "0.5.13",
    "qs": "6.9.1",
    "sha3": "2.1.1",
    "uuidv4": "6.0.0"
  },
