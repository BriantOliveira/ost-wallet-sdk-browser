{
  "name": "ost-wallet-sdk-browser",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev-mappy-server": "webpack-dev-server --config webpack.mappy.com.js --env.NODE_ENV=dev",
    "dev-ostsdk-server": "webpack-dev-server --config webpack.sdk-mappy.ostwalletsdk.com.js --env.NODE_ENV=dev",
    "dev-ostkm-server": "webpack-dev-server --config webpack.km-mappy.ostwalletsdk.com.js --env.NODE_ENV=dev",
    "dev-ost-scripts-server": "webpack-dev-server --config webpack.ost-scripts.js --env.NODE_ENV=dev",
    "dev-servers": "concurrently \"npm run dev-mappy-server\" \"npm run dev-ostsdk-server\" \"npm run dev-ostkm-server\" \"npm run dev-ost-scripts-server\"",
    "d-sdk-w": "webpack-dev-server --open --config webpack.mappy.com.js --env.NODE_ENV=dev",
    "b-sdk-w": "webpack --config webpack.mappy.com.js --env.NODE_ENV=prod",
    "d-sdk": "webpack-dev-server --open --config webpack.sdk-mappy.ostwalletsdk.com.js --env.NODE_ENV=dev",
    "b-sdk": "webpack --config webpack.sdk-mappy.ostwalletsdk.com.js --env.NODE_ENV=prod",
    "d-sdk-km": "webpack-dev-server --open --config webpack.km-mappy.ostwalletsdk.com.js --env.NODE_ENV=dev",
    "b-sdk-km": "webpack --config webpack.km-mappy.ostwalletsdk.com.js --env.NODE_ENV=prod",
    "develop-all": "concurrently \"npm run d-sdk-w\" \"npm run d-sdk\" \"npm run d-sdk-km\"",
    "sdkWallet": "npm run d-sdk-w",
    "run-app": "node src/AppServer/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshayraje/ost-browser-sdk.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/akshayraje/ost-browser-sdk#readme",
