    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.0.2",
    "css-loader": "3.4.2",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "1.1.2",
